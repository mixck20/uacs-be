{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/app.js"
    },
    {
      "source": "/(.*)",
      "destination": "/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}